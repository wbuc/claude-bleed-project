{
    "$schema": "https://json.schemastore.org/claude-code-settings.json",
    "mcpServers": {
      "chrome-devtools": {
        "command": "npx",
        "args": ["-y", "chrome-devtools-mcp@latest"]
      },
      "brave-search": {
        "command": "npx",
        "args": ["-y", "@anthropic-ai/mcp-server-brave-search"],
        "env": {
          "BRAVE_API_KEY": "YOUR_BRAVE_API_KEY_HERE"
        }
      }
    }
  }